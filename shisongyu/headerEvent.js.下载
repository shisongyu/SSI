$('#emailInputMb').val(data.message);}});});function getSearchCookie(name){return localStorage.getItem(name);}
function searchCookie(search){if($('#isStore').length>=1||$('#isStoremb').length>=1){return false;}
(search.length>10)?val=search.substr(0,10):val=search;var Queryarr=new Array();if(getSearchCookie("Querytext")==null){Queryarr[0]=val;}else{var Queryarr=JSON.parse(getSearchCookie("Querytext"));for(var i=0;i<Queryarr.length;i++){if(Queryarr[i]==val){Queryarr.splice(i,1);break;}}
Queryarr.unshift(val);}
localStorage.setItem("Querytext",JSON.stringify(Queryarr));}
$('#searchInputPc').focus(function(){var clear=$.trim($(this).attr('want_clear'));if(clear==1){$(this).val('');$(this).attr('want_clear','0');$(this).css('color','');}});function pcSearchStart(){var search_error=$('#search_error').val();var search_value=$.trim($('#searchInputPc').val());if(search_value==''||search_value==search_error){$('#searchInputPc').val(search_error);$('#searchInputPc').css('color','rgb(160,0,0)');$('#searchInputPc').attr('want_clear','1');return false;}
