function setCookie(name,value,time)
{var strsec=getsec(time);var exp=new Date();exp.setTime(exp.getTime()+strsec*1);var host=window.location.host;host=host.indexOf('cn');if(host>-1){document.cookie=name+"="+escape(value)+";domain=.gap.cn;expires="+exp.toGMTString()+";path=/";}else{document.cookie=name+"="+escape(value)+";domain=.gap.hk;expires="+exp.toGMTString()+";path=/";}}
function getsec(str)
{var str1=str.substring(1,str.length)*1;var str2=str.substring(0,1);if(str2=="s")
{return str1*1000;}
else if(str2=="h")
{return str1*60*60*1000;}
else if(str2=="d")
{return str1*24*60*60*1000;}}
function delCookie(name)
{var exp=new Date();exp.setTime(exp.getTime()-1);var cval=getCookie(name);if(cval!=null)
document.cookie=name+"="+cval+";domain=.gap.cn;expires="+exp.toGMTString()+";path=/";}
function getCookie(name){var cookieArray=document.cookie.split("; ");for(var i=0;i<cookieArray.length;i++){var arr=cookieArray[i].split("=");if(arr[0]==name){return unescape(arr[1]);}}
return null;}
var create_flat=false;var create_sent_flat=false;var create_website=$('#createWebsite').val();if(create_website==undefined){create_website='CN';}
if(create_website=='HK_en'){var create_warning_word='Required field,please enter a valid 11-digit mobile phone number,please enter a valid 8-digit mobile phone number,please enter a valid 10-digit mobile phone number,Password must be 6-16 characters,密码必须为6～16个英文字母加数字或符号的组合,Password must to be 6-16 characters, containing letters and numbers or letters and special characters, and must be the same as the password you entered,Please enter the 5 digit verification code,Wrong verification code,Send code,Resend verification code,Are you sure you don`t want to receive fashion news from Gap?,Please enter the number';}else{var create_warning_word='这是必填项,请输入11位有效手机号码,请输入8位有效手机号码,请输入10位有效手机号码,密码必须为6～16个字符,密码必须为6～16个英文字母加数字或符号的组合,密码必须为6～16个英文字母加数字或符号的组合,必须与输入的密码相同,请填写5位数字验证码,验证码错误,发送验证码,重新发送,您确定不接受盖璞向您推荐的各类时尚资讯？,请先填写手机号';}
try{if(create_website!='CN'&&convert&&typeof(convert)==="function"){create_warning_word=convert(1,create_warning_word);}}catch(e){}
create_warning_word=create_warning_word.split(",");function createMobileValue(){var mobile=$.trim($('#createMobile').val());var HKMmatch=/^[5|6|9]\d{7}$/;var TWMmatch=/^09\d{8}$/;var CNMmatch=/^(1[3|4|5|6|7|8])\d{9}$/;if(mobile==''){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(create_warning_word['0']).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});return false;}else if(create_website=='CN'&&!CNMmatch.test(mobile)){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(create_warning_word['1']).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});return false;}else if(create_website=='HK'&&!HKMmatch.test(mobile)){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(create_warning_word['2']).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});return false;}else if(create_website=='TW'&&!TWMmatch.test(mobile)){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(create_warning_word['3']).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});return false;}else if(create_website=='HK_en'&&!HKMmatch.test(mobile)){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(create_warning_word['3']).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});return false;}else{$.ajax({type:"GET",async:false,dataType:'json',url:"/customer/account/isMobileExist",data:{mobile:mobile},success:function(msg){if(msg.status=='error'){$('#createRightMobile').css({display:'block'});$('#createLeftMobile').html(msg.message).css("display","block");$("#createRightMobile").delay(3000).fadeOut();$("#createLeftMobile").delay(3000).fadeOut();$('#createMobileLogin').css({display:'none'});create_flat=false;}else if(msg.status=='exist'){$('#createRightMobile').css({display:'none'});$('#createClicklogin').css('display','block');$('#createMobileLogin').css({display:'none'});create_flat=false;}else if(msg.status=='noMobile'){$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'none'});$('#createMobileLogin').css({display:'block'});create_flat=false;}else{$('#createPhotoSubmitSent').css('backgroundColor','rgb(0,32,96)');$('#createClicklogin').css('display','none');$('#createRightMobile').css({display:'none'});$('#createMobileLogin').css({display:'none'});create_flat=true;}}});return create_flat;}}
function createPassValue(){var pass=$.trim($('#createPassword').val());var passreg=/(?!^(\d+|[a-zA-Z]+|[~!@#$%^&*?]+)$)^[\w~!@#$%\^&*?]{6,16}$/;if(pass==''){$('#createInfoPassword').css({display:'block'});$('#createLeftPassword').html(create_warning_word['0']).css("display","block");$('#createInfoPassword').delay(3000).fadeOut();$("#createLeftPassword").delay(3000).fadeOut();return false;}else if(pass.length<6){$('#createInfoPassword').css({display:'block'});$('#createLeftPassword').html(create_warning_word['4']).css("display","block");$('#createInfoPassword').delay(3000).fadeOut();$("#createLeftPassword").delay(3000).fadeOut();return false;}else if(!passreg.test(pass)){$('#createInfoPassword').css({display:'block'});$('#createLeftPassword').html(create_warning_word['5']).css("display","block");$('#createInfoPassword').delay(3000).fadeOut();$("#createLeftPassword").delay(3000).fadeOut();return false;}else{$('#createInfoPassword').css({display:'none'});$('#createLeftPassword').html('');return true;}}
