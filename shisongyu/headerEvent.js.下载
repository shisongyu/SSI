function add_prom(data,group_id){var now=new Date();var nowString=''+now.getFullYear()+(now.getMonth()>8?(now.getMonth()+1):"0"+(now.getMonth()+1))+(now.getDate()>9?now.getDate():('0'+now.getDate()))+(now.getHours()>9?now.getHours():('0'+now.getHours()))+(now.getMinutes()>9?now.getMinutes():('0'+now.getMinutes()))+(now.getSeconds()>9?now.getSeconds():('0'+now.getSeconds()));var group_id='0';if(getCookie('CGI')){group_id=String(parseInt(getCookie('CGI')));}
$.each(data,function(product_id,salerules){if($('.prom_product_'+product_id).length){var arrLabel=new Array();$.each(salerules,function(salerule_id,salerule){var group_ids=salerule.group_id.split(',');if((group_ids.indexOf(group_id)!=-1)&&nowString>=salerule.from_date&&nowString<=salerule.to_date){arrLabel.push(salerule.label);}});if(arrLabel.length>0){var show_prom_label=$('#show_prom_label').val();var prom_label=$('#show_prom_label').attr('title');if(show_prom_label=='true'){$('.mb_prom_label.prom_product_'+product_id).html(arrLabel.join(',')+"<br/><span class='salesRuleMb'>("+prom_label+')</span>');$('#pc_prom_label.prom_product_'+product_id).html(arrLabel.join(',')+"<br/><span class='salesRulePc'>("+prom_label+')</span>');}else{$('.prom_product_'+product_id).html(arrLabel.join(','));}}}});}
