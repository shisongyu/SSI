function fspassCheck(){var password=$.trim($('#fspassPassword').val());var confirmation=$.trim($('#fspassConfirmation').val());var passMatch=/(?!^(\d+|[a-zA-Z]+|[~!@#$%^&*?]+)$)^[\w~!@#$%\^&*?]{6,16}$/;if(password==''){$('#fspassInfoPassword').css({color:'rgb(160,0,0)'});return false;}else if(!passMatch.test(password)){$('#fspassInfoPassword').css({color:'rgb(160,0,0)'});return false;}else if(confirmation==''){$('#fspassInfoPassword').css({color:'rgb(120,120,120)'});$('#fspassUpPassword').css({color:'rgb(160,0,0)'});return false;}else if(password!=confirmation){$('#fspassInfoPassword').css({color:'rgb(120,120,120)'});$('#fspassUpPassword').css({color:'rgb(160,0,0)'});return false;}else{$('#fspassInfoPassword').css({color:'rgb(120,120,120)'});$('#fspassUpPassword').css({color:'rgb(120,120,120)'});return true;}}
var register_website=$('#registerWebsite').val();var register_warning_word='这是必填项,请输入11位有效手机号码,请输入8位有效手机号码,请输入10位有效手机号码,密码由6-16个字符组成，请使用英文字母加数字或符号的组合密码,请再输入一遍您上面输入的密码,请填写5位数字验证码,验证码错误,发送验证码,重新发送,请先填写手机号';try{if(register_website!='CN'&&convert&&typeof(convert)==="function"){register_warning_word=convert(1,register_warning_word);}}catch(e){}
register_warning_word=register_warning_word.split(",");var register_flat=true;var register_sent_flat=false;function registerMobileValue(){var mobile=$.trim($('#registerMobile').val());var HKMmatch=/^[5|6|9]\d{7}$/;var TWMmatch=/^09\d{8}$/;var CNMmatch=/^(1[3|4|5|6|7|8])\d{9}$/;if(mobile==''){$('#registerRightMobile').show();$('#registerLeftMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(register_warning_word['0']);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');register_flat=false;}else if(register_website=='CN'&&!CNMmatch.test(mobile)){$('#registerRightMobile').show();$('#registerLeftMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(register_warning_word['1']);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');register_flat=false;}else if(register_website=='HK'&&!HKMmatch.test(mobile)){$('#registerRightMobile').show();$('#registerLeftMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(register_warning_word['2']);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');register_flat=false;}else if(register_website=='TW'&&!TWMmatch.test(mobile)){$('#registerRightMobile').show();$('#registerLeftMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(register_warning_word['3']);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');register_flat=false;}else{$.ajax({type:"GET",async:false,dataType:'json',url:"/customer/account/isMobileExist",data:{mobile:mobile},success:function(msg){if(msg.status=='error'){$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');$('#registerRightMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(msg.message);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();register_flat=false;}else if(msg.status=='exist'){$('#registerPhotoSubmitSent').css('backgroundColor','rgb(221,221,221)');$('#registerRightMobile').css({display:'inline',color:'rgb(160,0,0)'}).html(msg.message);$("#registerRightMobile").delay(3000).fadeOut();$("#registerLeftMobile").delay(3000).fadeOut();register_flat=false;}else{$('#registerPhotoSubmitSent').css('backgroundColor','rgb(0,32,96)');$('#registerRightMobile').css({display:'none'}).html('');register_flat=true;}}});}
return register_flat;}
function registerPassValue(){var pass=$.trim($('#registerPassword').val());var passreg=/(?!^(\d+|[a-zA-Z]+|[~!@#$%^&*?]+)$)^[\w~!@#$%\^&*?]{6,16}$/;if(pass==''){$('#registerInfoPassword').css({color:'rgb(160,0,0)'});return false;}else if(pass.length<6){$('#registerInfoPassword').css({color:'rgb(160,0,0)'});return false;}else if(!passreg.test(pass)){$('#registerInfoPassword').css({color:'rgb(160,0,0)'});return false;}else{$('#registerInfoPassword').css({color:'rgb(153,153,153)'});return true;}}
function registerRepassValue(){var pass=$.trim($('#registerPassword').val());var repass=$('#registerConfirmation').val();var passreg=/(?!^(\d+|[a-zA-Z]+|[~!@#$%^&*?]+)$)^[\w~!@#$%\^&*?]{6,16}$/;if(repass==''){$('#registerInfoConfirmpassword').html(register_warning_word['4']).css({color:'rgb(160,0,0)',display:'inline'});return false;}else if(!passreg.test(repass)){$('#registerInfoConfirmpassword').html(register_warning_word['4']).css({color:'rgb(160,0,0)',display:'inline'});return false;}else if(pass!=repass){$('#registerInfoConfirmpassword').html(register_warning_word['5']).css({color:'rgb(160,0,0)',display:'inline'});return false;}else{$('#registerInfoConfirmpassword').html(register_warning_word['5']).css({color:'rgb(153,153,153)',display:'inline'});return true;}}
