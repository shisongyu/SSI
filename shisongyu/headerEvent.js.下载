function goToCart(){ajaxLoading('gotoCartMb','s','#FFF',true);url=$('input[name="cartType"]:checked').val();location.href=url;};(function($){accountRender();})(jQuery);function accountRender(){var cookieAccountName=getCookie('CUN');var cookieUserGroupId=getCookie('CGI');var c_start=document.cookie.indexOf(cookieAccountName);if(c_start==-1){$('#not_login').css('display','block');$('#not_login_up').css('display','none');$('#has_logged_on').css('display','none');$('#has_logged_on_up').css('display','none');$('.loginAfterMb').css('display','none');$('.loginBeforeMb').css('display','block');$('.dropdown-trigger').click(function(event){var isDropShowerHide=$(".dropdown-content").css("display")=="none"?true:false;if(isDropShowerHide){$(".dropdown-content").show();$("body").one("click",function(eve){$(".dropdown-content").hide();eve.stopPropagation();});event.stopPropagation();}else{$(".dropdown-content").css('display','block');}});}else{var HKMmatch=/^[5|6|9]\d{7}$/;var TWMmatch=/^09\d{8}$/;var CNMmatch=/^(1[3|4|5|6|7|8])\d{9}$/;$('#not_login').css('display','none');$('#not_login_up').css('display','none');$('#has_logged_on').css('display','block');$('#has_logged_on_up').css('display','none');$('.loginBeforeMb').css('display','none');$('.loginAfterMb').css('display','block');$('.loginRegisterPc1').click(function(event){var isDropShowerHide=$(".loginRegisterOpenPc1").css("display")=="none"?true:false;if(isDropShowerHide){$(".loginRegisterOpenPc1").show();$("body").one("click",function(eve){$(".loginRegisterOpenPc1").hide();eve.stopPropagation();});event.stopPropagation();}else{$(".loginRegisterOpenPc1").css('display','table');}});if(HKMmatch.test(cookieAccountName)){cookieAccountName=cookieAccountName.substring(0,3)+'***'+cookieAccountName.substring(6,8);}else if(TWMmatch.test(cookieAccountName)){cookieAccountName=cookieAccountName.substring(0,3)+'***'+cookieAccountName.substring(7,10);}else if(CNMmatch.test(cookieAccountName)){cookieAccountName=cookieAccountName.substring(0,3)+'*****'+cookieAccountName.substring(8,11);}
if(cookieUserGroupId==8||cookieUserGroupId==2||cookieUserGroupId==11||cookieUserGroupId==13){$('#loginName').html(" "+cookieAccountName);$('#logNameMb').html(" "+cookieAccountName);}else{$('#loginName').html(" "+cookieAccountName);$('#logNameMb').html(" "+cookieAccountName);}}}
function checkSearchValue(){var input_text=jQuery.trim(jQuery("#search").val());alert(input_text);if(input_text=='请输入关键字'){return false;}else{var input_text=jQuery.trim(jQuery("#search").val());if(input_text==''){jQuery("#search").css('color','red').val('请输入关键字');return false;}}}
